<?xml version="1.0" encoding="UTF-8"?>
<ti:app xmlns:ti="http://ti.appcelerator.org">
    <id>it.ziriziri.mobile</id>
    <name>ZiriZiri2</name>
    <version>1.0</version>
    <publisher>adrianomassi</publisher>
    <url>http://</url>
    <description>ZiriZiri Mobile</description>
    <copyright>2015 by adrianomassi</copyright>
    <icon>appicon.png</icon>
    <fullscreen>false</fullscreen>
    <navbar-hidden>false</navbar-hidden>
    <analytics>false</analytics>
    <guid>48f3b401-3ec8-4f0e-af4b-6e049aa83166</guid>
    <!--<guid>48f3b401-3ec8-4f0e-af4b-6e049aa83166</guid>-->
    <property name="ti.ui.defaultunit" type="string">dp</property>
    <ios>
        <plist>
            <dict>
                <key>UISupportedInterfaceOrientations~iphone</key>
                <array>
                    <string>UIInterfaceOrientationPortrait</string>
                </array>
                <key>UISupportedInterfaceOrientations~ipad</key>
                <array>
                    <string>UIInterfaceOrientationPortrait</string>
                    <string>UIInterfaceOrientationPortraitUpsideDown</string>
                    <string>UIInterfaceOrientationLandscapeLeft</string>
                    <string>UIInterfaceOrientationLandscapeRight</string>
                </array>
                <key>UIRequiresPersistentWiFi</key>
                <false/>
                <key>UIPrerenderedIcon</key>
                <false/>
                <key>UIStatusBarHidden</key>
                <false/>
                <key>UIStatusBarStyle</key>
                <string>UIStatusBarStyleDefault</string>
            </dict>
        </plist>
    </ios>
    <android xmlns:android="http://schemas.android.com/apk/res/android">
        <manifest android:installLocation="preferExternal"
            android:versionCode="10" android:versionName="2.1.4" package="it.ziriziri.mobile">
            <!--<uses-sdk android:minSdkVersion="14" android:targetSdkVersion="18"/>-->
            <uses-sdk android:minSdkVersion="14" android:targetSdkVersion="19"/>
            <supports-screens android:anyDensity="true"/>
            <!-- Allows the API to download data from Google Map servers -->
            <uses-permission android:name="android.permission.INTERNET"/>
            <!-- Allows the API to cache data -->
            <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
            <!-- Use GPS for device location -->
            <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
            <!-- Use Wi-Fi or mobile connection for device location -->
            <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
            <!-- Allows the API to access Google web-based services -->
            <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES"/>
            <!-- Specify OpenGL ES 2.0 as a requirement -->
            <uses-feature android:glEsVersion="0x00020000" android:required="true"/>
            <!-- Replace <com.domain.appid> with your application ID -->
            <uses-permission android:name="it.ziriziri.mobile.permission.MAPS_RECEIVE"/>
            <permission
                android:name="it.ziriziri.mobile.permission.MAPS_RECEIVE" android:protectionLevel="signature"/>
            <!--<application android:theme="@style/Theme.ZiriZiriTheme">-->
            <application android:theme="@style/Theme.Ziriziri">
                <meta-data
                    android:name="com.google.android.maps.v2.API_KEY" android:value="AIzaSyBy40Xya43EQmG6j-kIu5Q76IW3CE_53Yc"/>
            </application>
        </manifest>
    </android>
    <mobileweb>
        <precache/>
        <splash>
            <enabled>true</enabled>
            <inline-css-images>true</inline-css-images>
        </splash>
        <theme>default</theme>
    </mobileweb>
    <modules>
    <module platform="android">com.alcoapps.actionbarextras</module>
    <module platform="iphone">ti.imagefactory</module>
    <module platform="android">ti.imagefactory</module>
    <module platform="iphone">ti.map</module>
    <module platform="android">ti.map</module>
    <module platform="android" version="1.0.0">ti.zz.api</module>
    <module platform="iphone" version="1.0.0">ti.zz.api</module>
    <module platform="android" version="0.4.1">com.rkam.swiperefreshlayout</module>
  </modules>
    <deployment-targets>
        <target device="android">true</target>
        <target device="blackberry">false</target>
        <target device="ipad">true</target>
        <target device="iphone">true</target>
        <target device="mobileweb">false</target>
    </deployment-targets>
    <sdk-version>3.5.0.GA</sdk-version>
    <plugins>
        <plugin version="1.0">ti.alloy</plugin>
    </plugins>
</ti:app>
